<Window x:Class="HUD_Installer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Select the components you want to install!" Height="480" Width="711" Loaded="Window_Loaded">
    <DockPanel>
        <DockPanel DockPanel.Dock="Top">
            <Label Content="Preset:" DockPanel.Dock="Left" />
            <ComboBox Name="presets" DockPanel.Dock="Right">
                <ComboBoxItem Content="hyk" />
            </ComboBox>
        </DockPanel>
        <StatusBar Height="23" DockPanel.Dock="Bottom">
            <StatusBarItem>
                <ProgressBar Name="progress" Width="120" Height="13"/>
            </StatusBarItem>
        </StatusBar>
        <TabControl Name="tabs" DockPanel.Dock="Bottom">
        </TabControl>
    </DockPanel>
</Window>
